{
  "name": "yoobic-ng6",
  "version": "6.0.16",
  "license": "MIT",
  "scripts": {
    "build": "node --max_old_space_size=8096 $(npm bin)/ng build",
    "build:all": "npm run build:libs && npm run build:stencil && npm run build:app && npm run build:operations",
    "build:all:prod": "npm run build:libs:prod && npm run build:app:prod && npm run build:operations:prod",
    "build:app": "npm run build -- --project app-common-base && npm run build -- --project app-common-mobile && npm run build -- --project app-common-web ",
    "build:app:prod": "npm run build -- --project app-common-base --prod && npm run build -- --project app-common-mobile --prod && npm run build -- --project app-common-web --prod",
    "build:doc": "npm run copy:design-system && ./design-system/tools/stylemark/bin/stylemark -i ./design-system/stencil -c ./design-system/doc/config/stylemark.yml -o ./design-system/doc/dist",
    "build:libs": "npm run build -- --project interfaces && npm run build -- --project common && npm run build -- --project translate && npm run build -- --project data-core && npm run build -- --project data-form && npm run build -- --project data-live",
    "build:libs:prod": "npm run build -- --project interfaces --prod && npm run build -- --project common --prod && npm run build -- --project translate && npm run build -- --project data-core --prod && npm run build -- --project data-form --prod && npm run build -- --project data-live --prod",
    "build:operations": "npm run build -- --project operations-data && npm run build -- --project operations-common-base && npm run build -- --project operations-common-mobile && npm run build -- --project operations-common-web",
    "build:operations:full": "npm run build:operations:prod && npm run build -- --project operations-web --prod && npm run build -- --project operations-mobile --prod",
    "build:operations:prod": "npm run build -- --project operations-data --prod && npm run build -- --project operations-common-base --prod && npm run build -- --project operations-common-mobile --prod && npm run build -- --project operations-common-web --prod",
    "build:stencil": "npm run build -- --project interfaces &&  $(npm bin)/stencil build --dev --es5 --config=\"design-system/stencil/stencil.config.js\"",
    "build:stencil:es6": "npm run build -- --project interfaces &&  $(npm bin)/stencil build --dev --config=\"design-system/stencil/stencil.config.js\"",
    "build:stencil:only": "$(npm bin)/stencil build --dev --es5 --config=\"design-system/stencil/stencil.config.js\"",
    "build:stencil:prod": "npm run build -- --project interfaces && $(npm bin)/stencil build --prerender --config=\"design-system/stencil/stencil.config.js\"",
    "e2e": "ng e2e",
    "init": "npm i && npm run build:stencil && npm run build:libs && npm run build:app",
    "lint": "npm run lint:stencil && npm run lint:ng",
    "lint:ng": "ng lint",
    "lint:stencil": "tslint  -p design-system/stencil/tsconfig.json -c ./tslint.json",
    "ng": "ng",
    "reset:modules": "rm -rf node_modules && npm i",
    "serve": "node --max_old_space_size=8096 $(npm bin)/ng serve",
    "serve:operations": "$(npm bin)/concurrently --kill-others \"ng serve --project operations-mobile\"  \"ng serve --project operations-web\"",
    "serve:operations:mobile": "ng serve --project operations-mobile",
    "serve:operations:web": "ng serve --project operations-web",
    "serve:stencil": "npm run build:stencil && npm run copy:to:www && npm run stencil:dev:server",
    "stencil:watch:es6": "$(npm bin)/stencil build --dev --watch --config=\"design-system/stencil/stencil.config.js\"",
    "update": "npm i && npm run build:libs"
  },
  "private": true,
